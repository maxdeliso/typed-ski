{
  "all": true,
  "include": [
    "lib/**/*.ts"
  ],
  "exclude": [
    "**/*.d.ts",
    "test/**",
    "build/**",
    "node_modules/**"
  ],
  "reporter": [
    "text",
    "html"
  ],
  "check-coverage": true,
  "lines": 80,
  "statements": 80,
  "functions": 75,
  "branches": 70,
  "report-dir": "./coverage"
}
